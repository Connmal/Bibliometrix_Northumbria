@article{ WOS:000805264400001,
Author = {Kelly, Matthew},
Title = {Habitat Protection, Ideology and the British Nature State: The Politics
   of the Wildlife and Countryside Act 1981{*}},
Journal = {ENGLISH HISTORICAL REVIEW},
