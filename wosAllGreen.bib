@article{ WOS:000819823500092,
Author = {Hamad, Rebeen Ali and Yang, Longzhi and Woo, Wai Lok and Wei, Bo},
Title = {Cross-Domain Activity Recognition Using Shared Representation in Sensor
   Data},
Journal = {IEEE SENSORS JOURNAL},
Year = {2022},
