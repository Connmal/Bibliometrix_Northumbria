Author = {Zong, Yan and Dai, Xuewu and Wei, Zhuangkun and Zou, Mengbang and Guo,
   Weisi and Gao, Zhiwei},
Title = {Robust Time Synchronization for Industrial Internet of Things by H-8
   Output Feedback Control},
