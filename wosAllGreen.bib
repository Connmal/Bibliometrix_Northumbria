@inproceedings{ WOS:000886926200094,
Author = {Mansour, Amin Saatloo and Mudiyanselage, Manthila Wijesooriya and
   Mirzaei, Mohammad Amin and Mehrabi, Abbas and Marzband, Mousa and Aslam,
   Nauman},
Book-Group-Author = {IEEE},
Title = {Risk-Averse Decentralized Optimal Scheduling of a Virtual Energy Hub
   Plant Equipped with Multi Energy Conversion Facilities in Energy Markets},
