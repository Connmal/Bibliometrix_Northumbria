@article{ WOS:000730258600014,
Author = {Wilson, Caroline and Broughan, Christine and Daly, Guy},
Title = {Case Study: Decolonising the Curriculum - An Exemplification},
Journal = {SOCIAL POLICY AND SOCIETY},
Year = {2022},
Volume = {21},
