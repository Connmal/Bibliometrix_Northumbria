@article{ WOS:000742194000001,
Author = {Shalvey, T. P. and Shiel, H. and Hutter, O. S. and Zoppi, G. and Bowen,
   L. and Dhanak, V. R. and Major, J. D.},
Title = {Sodium Fluoride Doping Approach to CdTe Solar Cells},
Journal = {ACS APPLIED ENERGY MATERIALS},
Year = {2022},
Volume = {5},
