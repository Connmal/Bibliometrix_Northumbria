@article{ WOS:000812536000069,
Author = {Wang, Yinlu and Chen, Ming and Pan, Cunhua and Wang, Kezhi and Pan,
   Yijin},
Title = {Joint Optimization of UAV Trajectory and Sensor Uploading Powers for
   UAV-Assisted Data Collection in Wireless Sensor Networks},
Journal = {IEEE INTERNET OF THINGS JOURNAL},
Year = {2022},
Volume = {9},
