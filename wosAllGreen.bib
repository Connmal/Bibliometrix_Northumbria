@article{ WOS:000805787700009,
Author = {Spaa, Anne and Spencer, Nick and Durrant, Abigail and Vines, John},
Title = {Creative and collaborative reflective thinking to support policy
   deliberation and decision making},
Journal = {EVIDENCE \& POLICY},
Year = {2022},
Volume = {18},
