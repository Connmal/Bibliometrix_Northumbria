@article{ WOS:000773145100001,
Author = {Kele, Juliet Elizabeth and Cassell, Catherine and Ford, Jacqueline and
   Watson, Kathryn},
Title = {Intersectional identities and career progression in retail: The
   experiences of minority-ethnic women},
Journal = {GENDER WORK AND ORGANIZATION},
