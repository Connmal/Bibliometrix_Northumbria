@article{ WOS:000803129500039,
Author = {Bo, Wan and Liu, Bin and Liu, Juan and He, Xing-Dao and Yuan, Jinhui and
   Wu, Qiang},
Title = {Fiber Ring Laser Based on Side-Polished Fiber MZI for Enhancing
   Refractive Index and Torsion Measurement},
Journal = {IEEE SENSORS JOURNAL},
