@article{ WOS:000812536000040,
Author = {Li, Xingquan and Zhang, Chiya and Zhao, Rujun and He, Chunlong and
   Zheng, Hongxia and Wang, Kezhi},
Title = {Energy-Effective Offloading Scheme in UAV-Assisted C-RAN System},
Journal = {IEEE INTERNET OF THINGS JOURNAL},
Year = {2022},
