@article{ WOS:000874384500001,
Author = {Griffin, Paul},
Title = {Unemployed Workers' Centres (1978-): Spatial Politics,
   ``Non-Movement{''}, and the Making of Centres},
Journal = {ANTIPODE},
Year = {2023},
Volume = {55},
Number = {2},
